# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
Host wu247-prod
    HostName whatsup247.com
    User dominic
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa
Host wu247-dev
    HostName test.whatsup247.net
    User dominic
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

# Fig ssh integration. Keep at the bottom of this file.
Match all
  Include ~/.fig/ssh
